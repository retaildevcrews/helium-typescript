{
  "name": "helium",
  "version": "1.1.0",
  "description": "Azure App Service reference application",
  "main": "dist/server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "mocha test/unit/**/*.ts",
    "test:watch": "npm test -- -w",
    "test:coverage": "nyc mocha test/unit/**/*.ts",
    "test:e2e": "nyc mocha test/e2e/**/*.ts",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint --max-warnings=0 src/**/*.ts test/**/*.ts",
    "fix": "eslint --fix src/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retaildevcrews/helium-typescript.git"
  },
  "author": "aka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/retaildevcrews/helium-typescript/issues"
  },
  "homepage": "https://github.com/retaildevcrews/helium-typescript#readme",
  "dependencies": {
    "@azure/cosmos": "^3.7.4",
    "@azure/identity": "^1.0.3",
    "@azure/keyvault-secrets": "^4.0.4",
    "@azure/ms-rest-nodeauth": "^3.0.5",
    "applicationinsights": "^1.8.2",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "command-line-args": "^5",
    "command-line-usage": "^6",
    "http-status-codes": "^1.4.0",
    "inversify": "^5.0.1",
    "inversify-restify-utils": "^3.4.1",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "restify": "^8.5.1",
    "swagger-ui-dist": "^3.30.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.24",
    "@types/restify": "^8.4.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "bufferutil": "^4.0.1",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "gulp": "^4.0.2",
    "mocha": "^8.0.1",
    "mockery": "^2.1.0",
    "node-mocks-http": "^1.8.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "utf-8-validate": "^5.0.2"
  }
}
