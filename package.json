{
  "name": "helium",
  "version": "0.0.6",
  "description": "Azure App Service reference application",
  "main": "dist/server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "mocha test/unit/**/*.ts",
    "test:watch": "npm test -- -w",
    "test:coverage": "nyc mocha test/unit/**/*.ts",
    "test:e2e": "nyc mocha test/e2e/**/*.ts",
    "build": "tsc",
    "build:watch": "tsc -w",
    "docker-build": "docker build -t helium:canary .",
    "docker-run": "docker run -it -p 8080:8080 helium:canary",
    "lint": "eslint --max-warnings=0 src/**/*.ts test/**/*.ts",
    "fix": "eslint --fix src/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/helium.git"
  },
  "author": "aka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/helium/issues"
  },
  "homepage": "https://github.com/Microsoft/helium#readme",
  "dependencies": {
    "@azure/cosmos": "^3.6.2",
    "@azure/identity": "^1.0.2",
    "@azure/keyvault-secrets": "^4.0.2",
    "@azure/ms-rest-nodeauth": "^3.0.3",
    "applicationinsights": "^1.7.2",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "command-line-args": "^5",
    "command-line-usage": "^6",
    "http-status-codes": "^1.4.0",
    "inversify": "^5.0.1",
    "inversify-restify-utils": "^3.4.0",
    "reflect-metadata": "^0.1.13",
    "restify": "^8.5.1",
    "swagger-ui-dist": "^3.25.0",
    "uuid": "^7.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "@types/restify": "^8.4.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "bufferutil": "^4.0.1",
    "chai": "^4.2.0",
    "eslint": "^6",
    "mocha": "^7.1.0",
    "mockery": "^2.1.0",
    "node-mocks-http": "^1.8.1",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.2",
    "utf-8-validate": "^5.0.2"
  }
}
